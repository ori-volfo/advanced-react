    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "babel-polyfill": "^6.26.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "webpack": "^4.44.1"
